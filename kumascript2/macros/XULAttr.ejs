<%
var lang = env.locale;
var anch = "a-" + $0;
var dispApi = $0;
if ($1) {
  dispApi = $1;
}

var attrName = mdn.localString({
    "en-US":    "Attribute",
    "de":       "Attribute",
    "es":       "Attribute",
    "fr":       "Attributs",
    "ja":       "Attribute",
    "ko":       "Attribute",
    "pl":       "Atrybut",
    "zh-CN":    "Attribute",
    "ru":       "Атрибуты"
});

var newPath = mdn.localString({
    "en-US":    "Mozilla/Tech/XUL/Attribute",
    "de":       "Mozilla/Tech/XUL/Attribute",
    "es":       "Mozilla/Tech/XUL/Attribute",
    "fr":       "Mozilla/Tech/XUL/Attributs",
    "ja":       "Mozilla/Tech/XUL/Attribute",
    "ko":       "Mozilla/Tech/XUL/Attribute",
    "pl":       "Mozilla/Tech/XUL/Atrybut",
    "zh-CN":    "Mozilla/Tech/XUL/Attribute",
    "ru":       "Mozilla/Tech/XUL/Атрибуты"
});

var destOldest = lang + '/docs/XUL:' + attrName + ':' + $0;
var destOld = lang + '/docs/XUL/' + attrName + '/' + $0;
var destNew = lang + "/docs/" + newPath + "/" + $0;

if (wiki.pageExists(destNew)) {
    %><code id="<%= anch %>"><%-(web.link(wiki.uri(destNew), dispApi))%></code><%
} else if (wiki.pageExists(destOld)) {
    %><code id="<%= anch %>"><%-(web.link(wiki.uri(destOld), dispApi))%></code><%
} else if (wiki.pageExists(destOldest)) {
    %><code id="<%= anch %>"><%-(web.link(wiki.uri(destOldest), dispApi))%></code><%
} else {
    %><code id="<%= anch %>"><%-(web.link(wiki.uri(destNew), dispApi))%></code><%
}%>
