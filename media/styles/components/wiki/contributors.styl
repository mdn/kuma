/* contributors block at the bottom of the article */
.contributors {
    set-smaller-font-size();
    color: #777;

    {$selector-icon} {
        bidi-style(margin-left, 0, margin-right, $icon-margin);
        bidi-style(margin-right, $icon-margin, margin-left, 0);
    }
}

.contributors-sub {
    padding: 4px 0;
}

@media print {

    .contributors {
        display: none;
    }
}
